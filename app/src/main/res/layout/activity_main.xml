<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <FrameLayout
        android:id="@+id/fragment"
        android:name="com.example.fragment_question4.Frag_1"
        android:layout_width="389dp"
        android:layout_height="272dp"
        android:layout_marginBottom="72dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.841"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_shoaib"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="shoaib"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_manish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="manish"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_shoaib" />

    <Button
        android:id="@+id/btn_ritik"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ritik"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_manish" />
</androidx.constraintlayout.widget.ConstraintLayout>